(executable
 (public_name capitalcommander)
 (name main)
 (libraries capitalcommander dream)
 (preprocess (pps lwt_ppx)))

(rule
 (targets error.ml)
  (deps error.eml.ml)
   (action (run dream_eml %{deps} --workspace %{workspace_root})))

(rule
 (targets version.ml)
  (deps version.eml.html)
   (action (run dream_eml %{deps} --workspace %{workspace_root})))
